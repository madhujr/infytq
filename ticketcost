def calculate_total_ticket_cost(no_of_adults, no_of_children):
    total_ticket_cost=0
    rate_per_adult=37550.0
    rate_per_child=1/3*(rate_per_adult)
    Total=no_of_adults*rate_per_adult+no_of_children*rate_per_child
    tax=7/100*(Total)
    total1=Total+tax
    discount=10/100*(total1)
    total_ticket_cost=total1-discount
    return total_ticket_cost
total_ticket_cost=calculate_total_ticket_cost(3,1)
print("Total Ticket Cost:",total_ticket_cost)
